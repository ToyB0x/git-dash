{
  "name": "@git-dash/web",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "NODE_ENV=production react-router build",
    "dev": "react-router dev",
    "start:preview": "vite preview",
    "//deploy:[env]": "currently cloudflare pages only support preview / production enviroment, so manually overwrite config file",
    "deploy:development": "pnpm build && cp wrangler-dev.toml wrangler.toml && wrangler pages deploy --branch main && cp wrangler-local.toml wrangler.toml",
    "deploy:staging": "pnpm build && cp wrangler-stg.toml wrangler.toml && wrangler pages deploy --branch main && cp wrangler-local.toml wrangler.toml",
    "deploy:production": "pnpm build && cp wrangler-prd.toml wrangler.toml && wrangler pages deploy --branch main && cp wrangler-local.toml wrangler.toml",
    "typecheck": "react-router typegen && tsc"
  },
  "dependencies": {
    "@assistant-ui/react": "0.10.42",
    "@git-dash/api": "workspace:*",
    "@git-dash/db": "workspace:*",
    "@git-dash/db-api": "workspace:*",
    "@git-dash/utils": "workspace:*",
    "@internationalized/date": "3.8.2",
    "@radix-ui/react-accordion": "1.2.11",
    "@radix-ui/react-checkbox": "1.3.3",
    "@radix-ui/react-dialog": "1.1.14",
    "@radix-ui/react-dropdown-menu": "2.1.15",
    "@radix-ui/react-hover-card": "1.1.14",
    "@radix-ui/react-label": "2.1.7",
    "@radix-ui/react-navigation-menu": "1.2.13",
    "@radix-ui/react-popover": "1.1.14",
    "@radix-ui/react-radio-group": "1.3.8",
    "@radix-ui/react-select": "2.2.6",
    "@radix-ui/react-slider": "1.3.6",
    "@radix-ui/react-slot": "1.2.3",
    "@radix-ui/react-switch": "1.2.6",
    "@radix-ui/react-tabs": "1.1.12",
    "@radix-ui/react-toast": "1.2.14",
    "@radix-ui/react-tooltip": "1.2.8",
    "@react-aria/datepicker": "3.15.0",
    "@react-router/node": "7.8.1",
    "@react-router/serve": "7.8.1",
    "@react-stately/datepicker": "3.15.0",
    "@remixicon/react": "4.6.0",
    "@tanstack/react-table": "8.21.3",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "drizzle-orm": "0.44.4",
    "firebase": "11.10.0",
    "isbot": "5.1.28",
    "logrocket": "9.0.2",
    "next-themes": "0.4.6",
    "react": "19.1.1",
    "react-day-picker": "8.10.1",
    "react-dom": "19.1.1",
    "react-markdown": "9.1.0",
    "react-router": "7.8.1",
    "recharts": "2.15.4",
    "sql.js": "1.13.0",
    "tailwind-merge": "2.6.0",
    "tailwind-variants": "0.3.1",
    "tailwindcss-animate": "1.0.7",
    "valibot": "1.1.0"
  },
  "devDependencies": {
    "@react-router/dev": "7.8.1",
    "@tailwindcss/forms": "0.5.10",
    "@tsconfig/strictest": "2.0.5",
    "@types/node": "22.17.2",
    "@types/react": "19.1.10",
